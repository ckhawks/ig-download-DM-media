{
    "name": "IG Downloader Chrome Extension",
    "version": "1.0",
    "description": "Build an Extension!",
    "devtools_page": "devtools.html",
    "permissions": ["storage", "unlimitedStorage", "downloads"],
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    }
}